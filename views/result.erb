
<div id="highlighted">
    <div class="container">
        <div class="row-fluid header">
            <h2 class="page-title"><span><a href="/./">ID入力 </a><span class="divider">/</span><a href="/./ranking/information?id=<%= @id %>"> キーワード入力 </a><span class="divider">/</span> 結果</h2>
        </div>
    </div>
</div>  

<div id="content">
  <div class="container"> 
    <div class="block services">
      <ul class="thumbnails">
        <li class="span12">
            <div class="span3">
                <p><img src="<%= @image_url2 %>" />
            </div>
            <div class="span9">
                <%= @artistName2 %></p>
            </div>
            <div class="span6">
                <table class="table">
                    <thread>
                        <tr><th>キーワード</th><th>順位</th>
                    </thread>
                    <tbody>
                        <% for i in 0..@ranks.size do %>
                        <tr><th><%= @keywords[0][i] %></th><th><%= @ranks[i] %></th>
                        <% end %>
                    </tbody>
                </table>
            </div>
        </li>
      </ul>
    </div>
  </div>
</div>